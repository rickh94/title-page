{
  "name": "frontend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "Apache 2.0",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "milligram": "^1.3.0",
    "prop-types": "latest",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-dropzone": "^10.2.1",
    "react-hanger": "^2.1.0",
    "sweetalert2": "^8.19.0"
  },
  "scripts": {
    "build": "rm -rf dist/* && parcel build --public-url /static src/index.html",
    "watch": "parcel watch --public-url /static src/index.html",
    "pretest": "npm run build",
    "debug": "npm run build && NODE_ENV=debug jest",
    "test": "jest",
    "test:all": "npm run build && jest -c jest.config.js",
    "test:unit": "jest -c jest.config.unit.js",
    "test:live": "npm run build && jest -c jest.config.live.js",
    "test:update": "jest -u -c jest.config.unit.js",
    "test:coverage": "jest -c jest.config.unit.js --coverage"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-stage-2": "^7.0.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@types/jest": "^24.0.23",
    "babel-eslint": "^10.0.3",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "concurrently": "^4.1.2",
    "eslint": "^6.7.2",
    "eslint-plugin-react": "^7.17.0",
    "faker": "^4.1.0",
    "fetch-mock": "^7.7.3",
    "jest": "^24.9.0",
    "jest-transform-stub": "^2.0.0",
    "node-fetch": "^3.1.1",
    "parcel-bundler": "^1.12.3",
    "puppeteer": "^1.19.0"
  }
}
